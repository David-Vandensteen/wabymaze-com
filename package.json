{
  "name": "wabymaze-com",
  "version": "1.0.0",
  "description": "Single web page for ***WabyMaze*** iOS game",
  "scripts": {
    "start": "npm run http-server",
    "mon": "npx -y nodemon --watch src -e html,js,svg,ico,png,jpg",
    "http-server": "npx -y http-server src/",
    "open-browser": "npx -y opener http://127.0.0.1:8080",
    "serve": "npm-run-all --parallel mon open-browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/David-Vandensteen/wabymaze-com.git"
  },
  "author": "David Vandensteen <dvandensteen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/David-Vandensteen/wabymaze-com/issues"
  },
  "homepage": "https://github.com/David-Vandensteen/wabymaze-com#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
